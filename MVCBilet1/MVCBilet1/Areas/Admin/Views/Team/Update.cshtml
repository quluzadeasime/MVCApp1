@using Core.Models
@model Team

<div class="container">
    <form method=post ,enctype="multipart/form-data">
        <div class="mb-3">
            <label asp-for="Fullname" class="form-label">Fullname </label>
            <input asp-for="Fullname" class="form-control">
            <span asp-validation-for="Fullname" style="color:red"> </span>
        </div>
        <div class="mb-3">
            <label asp-for="Position" class="form-label">Position</label>
            <input asp-for="Position" class="form-control">
            <span asp-validation-for="Position" style="color:red"> </span>

        </div>
        <div class="mb-3">
            <label asp-for="PhotoFile" class="form-label">PhotoFile</label>
            <input asp-for="PhotoFile" class="form-control">
            <span asp-validation-for="PhotoFile" style="color:red"> </span>

        </div>
        <div class="mb-3 form-check">
            <input type="checkbox" class="form-check-input" id="exampleCheck1">
            <label class="form-check-label" for="exampleCheck1">Check me out</label>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>